<h1>Dashboard</h1>

<%= form_for @shout do |form| %>
	<%= form.fields_for :content do |content_form| %>
		<%= content_form.text_field :body, placeholder: "Shout here!", required: true %>
	<% end %>
	<%= form.submit "Shout" %>
<% end %>

<%# When iterating through instance variables, do not use = / it will show database information  %>
<%= render @shouts %>
