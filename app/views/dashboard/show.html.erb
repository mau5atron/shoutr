<h1>Dashboard</h1>

<%= form_for @shout do |form| %>
	<%= form.text_field :body, placeholder: "Shout here!", required: true %>
	<%= form.submit "Shout" %>
<% end %>

<%# When iterating through instance variables, do not use = / it will show database information  %>
<% @shouts.each do |shout| %>
	<div>
		<%= shout.body %>
	</div>
<% end %>